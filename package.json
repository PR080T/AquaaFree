    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^10.16.4",
    "ioredis": "^5.3.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.1",
    "next": "13.4.12",
    "pg": "8.11.0",
    "rate-limiter-flexible": "2.3.9",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.3.5"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "next/babel"
          ]
        }
      ]
    }
  },
  "description": "A modern Next.js application for water distribution and brand promotion services, combining corporate social responsibility with effective marketing. Founded by Amogh Jain, we focus on community harmony and sustainable practices.",
  "main": "server.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PR080T/AquaaFree.git"
